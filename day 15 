#include <stdio.h>

int main() {
    int m, n;
    scanf("%d %d", &m, &n);

    int a[m][n];
    int sum = 0;

    // Read matrix and add diagonal elements
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &a[i][j]);
            if (i == j) {
                sum += a[i][j];
            }
        }
    }

    printf("%d", sum);
    return 0;
}